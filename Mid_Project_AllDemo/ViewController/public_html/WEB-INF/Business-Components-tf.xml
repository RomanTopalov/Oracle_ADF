<?xml version="1.0" encoding="windows-1251" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Business-Components-tf">
    <default-activity>initialization_Business_Components</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>InitializationBean</managed-bean-name>
      <managed-bean-class>com.view.InitializationBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__6">
      <managed-bean-name>ButtonActionBean</managed-bean-name>
      <managed-bean-class>com.view.ButtonActionBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <method-call id="initialization_Business_Components">
      <method>#{pageFlowScope.InitializationBean.initialization_Business_Components}</method>
      <outcome>
        <fixed-outcome>continue</fixed-outcome>
      </outcome>
    </method-call>
    <view id="business-component-v">
      <page>/business-component-v.jsff</page>
    </view>
    <task-flow-return id="ReturnToFirstPage">
      <outcome>
        <name>ReturnToFirstPage</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__2">
      <from-activity-id>initialization_Business_Components</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>continue</from-outcome>
        <to-activity-id>business-component-v</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>goToFirstPage</from-outcome>
        <to-activity-id>ReturnToFirstPage</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
